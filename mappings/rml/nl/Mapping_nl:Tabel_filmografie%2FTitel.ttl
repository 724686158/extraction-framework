@prefix wikidata: 	<http://www.wikidata.org/entity/> .
@prefix dc:    		<http://purl.org/dc/elements/1.1/> .


### Main TriplesMap
####################
<>      a                      rr:TriplesMap ;
        rml:logicalSource      <Titel/LogicalSource> ;
        rr:predicateObjectMap  <Titel/CorrespondingProperty> ,
			       <Titel/SimplePropertyMapping/0> ,
			       <Titel/SimplePropertyMapping/1> ;
        rr:subjectMap          <Titel/SubjectMap> .


### Main SubjectMap
####################
<Titel/SubjectMap>  a  rr:SubjectMap ;
        rr:class     wikidata:Q386724 ,
		     schema:CreativeWork ,
		     <http://dbpedia.org/ontology/Wikidata:Q11424> ,
		     dbo:Film ,
		     schema:Movie ,
		     dbo:Work ,
		     owl:Thing ;
        rr:template  "http://en.dbpedia.org/resource/{wikititle}" ;
        rr:termType  rr:IRI .


### LogicalSource
##################

<Titel/LogicalSource>
        a                         rml:LogicalSource ;
        rml:iterator              "Infobox" ;
        rml:referenceFormulation  ql:wikitext ;
        rml:source                "http://nl.wikipedia.org/wiki/Mapping_nl:Tabel_filmografie/Titel?oldid=28894" .


###########
# Mappings
###########

### Predicate Object Map
#########################
<Titel/SimplePropertyMapping/1>
        a             rr:PredicateObjectMap ;
        rdfs:comment  "A PredicateObjectMap of the main TriplesMap that defines the generation of the predicate and object of a triple. The object value will be the result of a SimplePropertyFunction." ;
        rdfs:label    "SimplePropertyMapping (PredicateObjectMap)." ;
        rr:objectMap  <Titel/SimplePropertyMapping/1/FTM> ;
        rr:predicate  dbo:title .

### Function Term Map
<Titel/SimplePropertyMapping/1/FTM>
        a                   fnml:FunctionTermMap ;
        fnml:functionValue  <Titel/SimplePropertyMapping/1/FTM/FV> ;
        rr:language         "nl" .

### Function Execution Mapping
<Titel/SimplePropertyMapping/1/FTM/FV>
        a                      rr:TriplesMap ;
        rdfs:comment           "TriplesMap that generates and returns execution triples that can be executed by the SimplePropertyFunction." ;
        rdfs:label             "SimplePropertyMapping TriplesMap" ;
        rml:logicalSource      <Titel/LogicalSource> ;
        rr:predicateObjectMap  <Titel/Function/SimplePropertyFunction> ,
			       <Titel/SimplePropertyMapping/1/FTM/FV/propertyParameterPOM> ,
			       <Titel/SimplePropertyMapping/1/FTM/FV/ontologyPropertyParameterPOM> ;
        rr:subjectMap          <Titel/SubjectMap/Function> .

### Parameters
<Titel/SimplePropertyMapping/1/FTM/FV/propertyParameterPOM>
        a             rr:PredicateObjectMap ;
        rdfs:comment  "A Predicate Object Map for generating and returning the predicate and/or object of a parameter triple." ;
        rdfs:label    "Parameter Predicate Object Map" ;
        rr:objectMap  <Titel/SimplePropertyMapping/1/FTM/FV/propertyParameterPOM/ObjectMap> ;
        rr:predicate  dbf:propertyParameter .

<Titel/SimplePropertyMapping/1/FTM/FV/ontologyPropertyParameterPOM>
        a             rr:PredicateObjectMap ;
        rdfs:comment  "A Predicate Object Map for generating and returning the predicate and/or object of a parameter triple." ;
        rdfs:label    "Parameter Predicate Object Map" ;
        rr:object     "title" ;
        rr:predicate  dbf:ontologyPropertyParameter .

### References
<Titel/SimplePropertyMapping/1/FTM/FV/propertyParameterPOM/ObjectMap>
        a              rr:ObjectMap ;
        rdfs:comment   "An Object Map for generating and returning the object of a parameter triple. This Object Map refers to a value of a key in an infoboxy with the rml:reference property." ;
        rdfs:label     "Parameter Object Map" ;
        rml:reference  "als" .


### Predicate Object Map
#########################
<Titel/SimplePropertyMapping/0>
        a             rr:PredicateObjectMap ;
        rdfs:comment  "A PredicateObjectMap of the main TriplesMap that defines the generation of the predicate and object of a triple. The object value will be the result of a SimplePropertyFunction." ;
        rdfs:label    "SimplePropertyMapping (PredicateObjectMap)." ;
        rr:objectMap  <Titel/SimplePropertyMapping/0/FTM> ;
        rr:predicate  dbo:originalTitle .

### Function Term Map
<Titel/SimplePropertyMapping/0/FTM>
        a                   fnml:FunctionTermMap ;
        fnml:functionValue  <Titel/SimplePropertyMapping/0/FTM/FV> ;
        rr:language         "nl" .

