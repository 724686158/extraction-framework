### Main TriplesMap
####################
<>      a                      rr:TriplesMap ;
        rml:logicalSource      <./Mapping_uk:VG_Reviews/LogicalSource> ;
        rr:predicateObjectMap  <./Mapping_uk:VG_Reviews/SimplePropertyMapping/0> ,
			       <./Mapping_uk:VG_Reviews/SimplePropertyMapping/1> ;
        rr:subjectMap          <./Mapping_uk:VG_Reviews/SubjectMap> .


### Main SubjectMap
####################
<./Mapping_uk:VG_Reviews/SubjectMap>
        a            rr:SubjectMap ;
        rr:class     dbo:Software ,
		     wikidata:Q7889 ,
		     wikidata:Q386724 ,
		     dbo:VideoGame ,
		     dbo:Work ,
		     owl:Thing ,
		     schema:CreativeWork ,
		     wikidata:Q7397 ;
        rr:template  "http://en.dbpedia.org/resource/{wikititle}" ;
        rr:termType  rr:IRI .


### LogicalSource
##################

<./Mapping_uk:VG_Reviews/LogicalSource>
        a                         rml:LogicalSource ;
        rml:iterator              "Infobox" ;
        rml:referenceFormulation  ql:wikitext ;
        rml:source                "http://uk.wikipedia.org/wiki/Mapping_uk:VG_Reviews?oldid=43805" .


###########
# Mappings
###########

### Predicate Object Map
#########################
<./Mapping_uk:VG_Reviews/SimplePropertyMapping/1>
        a             rr:PredicateObjectMap ;
        rdfs:comment  "A PredicateObjectMap of the main TriplesMap that defines the generation of the predicate and object of a triple. The object value will be the result of a SimplePropertyFunction." ;
        rdfs:label    "SimplePropertyMapping (PredicateObjectMap)." ;
        rr:objectMap  <./Mapping_uk:VG_Reviews/SimplePropertyMapping/1/FTM> ;
        rr:predicate  dbo:releaseDate .

### Function Term Map
<./Mapping_uk:VG_Reviews/SimplePropertyMapping/1/FTM>
        a                   fnml:FunctionTermMap ;
        fnml:functionValue  <./Mapping_uk:VG_Reviews/SimplePropertyMapping/1/FTM/FV> .

### Function Execution Mapping
<./Mapping_uk:VG_Reviews/SimplePropertyMapping/1/FTM/FV>
        a                      rr:TriplesMap ;
        rdfs:comment           "TriplesMap that generates and returns execution triples that can be executed by the SimplePropertyFunction." ;
        rdfs:label             "SimplePropertyMapping TriplesMap" ;
        rml:logicalSource      <./Mapping_uk:VG_Reviews/LogicalSource> ;
        rr:predicateObjectMap  <./Mapping_uk:VG_Reviews/Function/SimplePropertyFunction> ,
			       <./Mapping_uk:VG_Reviews/SimplePropertyMapping/1/FTM/FV/propertyParameterPOM> ,
			       <./Mapping_uk:VG_Reviews/SimplePropertyMapping/1/FTM/FV/ontologyPropertyParameterPOM> ;
        rr:subjectMap          <./Mapping_uk:VG_Reviews/SubjectMap/Function> .

### Parameters
<./Mapping_uk:VG_Reviews/SimplePropertyMapping/1/FTM/FV/propertyParameterPOM>
        a             rr:PredicateObjectMap ;
        rdfs:comment  "A Predicate Object Map for generating and returning the predicate and/or object of a parameter triple." ;
        rdfs:label    "Parameter Predicate Object Map" ;
        rr:objectMap  <./Mapping_uk:VG_Reviews/SimplePropertyMapping/1/FTM/FV/propertyParameterPOM/ObjectMap> ;
        rr:predicate  dbf:propertyParameter .

<./Mapping_uk:VG_Reviews/SimplePropertyMapping/1/FTM/FV/ontologyPropertyParameterPOM>
        a             rr:PredicateObjectMap ;
        rdfs:comment  "A Predicate Object Map for generating and returning the predicate and/or object of a parameter triple." ;
        rdfs:label    "Parameter Predicate Object Map" ;
        rr:object     "releaseDate" ;
        rr:predicate  dbf:ontologyPropertyParameter .

### References
<./Mapping_uk:VG_Reviews/SimplePropertyMapping/1/FTM/FV/propertyParameterPOM/ObjectMap>
        a              rr:ObjectMap ;
        rdfs:comment   "An Object Map for generating and returning the object of a parameter triple. This Object Map refers to a value of a key in an infoboxy with the rml:reference property." ;
        rdfs:label     "Parameter Object Map" ;
        rml:reference  "gamerev" .


