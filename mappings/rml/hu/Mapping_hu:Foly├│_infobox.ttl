### Main TriplesMap
####################
<./Mapping_hu:Foly%C3%B3_infobox>
        a                      rr:TriplesMap ;
        rml:logicalSource      <./Mapping_hu:Foly%C3%B3_infobox/LogicalSource> ;
        rr:predicateObjectMap  <./Mapping_hu:Foly%C3%B3_infobox/SimplePropertyMapping/7> ,
			       <./Mapping_hu:Foly%C3%B3_infobox/SimplePropertyMapping/5> ,
			       <./Mapping_hu:Foly%C3%B3_infobox/SimplePropertyMapping/6> ,
			       <./Mapping_hu:Foly%C3%B3_infobox/SimplePropertyMapping/3> ,
			       <./Mapping_hu:Foly%C3%B3_infobox/SimplePropertyMapping/4> ,
			       <./Mapping_hu:Foly%C3%B3_infobox/SimplePropertyMapping/1> ,
			       <./Mapping_hu:Foly%C3%B3_infobox/SimplePropertyMapping/0> ,
			       <./Mapping_hu:Foly%C3%B3_infobox/SimplePropertyMapping/2> ;
        rr:subjectMap          <./Mapping_hu:Foly%C3%B3_infobox/SubjectMap> .


### Main SubjectMap
####################
<./Mapping_hu:Foly%C3%B3_infobox/SubjectMap>
        a            rr:SubjectMap ;
        rr:class     dbo:BodyOfWater ,
		     schema:Place ,
		     owl:Thing ,
		     wikidata:Q47521 ,
		     wikidata:Q4022 ,
		     dbo:NaturalPlace ,
		     dbo:Stream ,
		     dbo:Location ,
		     dbo:River ,
		     schema:RiverBodyOfWater ,
		     schema:BodyOfWater ,
		     dbo:Place ;
        rr:template  "http://en.dbpedia.org/resource/{wikititle}" ;
        rr:termType  rr:IRI .


### LogicalSource
##################

<./Mapping_hu:Foly%C3%B3_infobox/LogicalSource>
        a                         rml:LogicalSource ;
        rml:iterator              "Infobox" ;
        rml:referenceFormulation  ql:wikitext ;
        rml:source                "http://hu.wikipedia.org/wiki/Mapping_hu:Foly%C3%B3_infobox?oldid=10484" .


