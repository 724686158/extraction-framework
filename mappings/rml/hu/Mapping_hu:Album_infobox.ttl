### Main TriplesMap
####################
<>      a                      rr:TriplesMap ;
        rml:logicalSource      <./Mapping_hu:Album_infobox/LogicalSource> ;
        rr:predicateObjectMap  <./Mapping_hu:Album_infobox/SimplePropertyMapping/15> ,
			       <./Mapping_hu:Album_infobox/SimplePropertyMapping/19> ,
			       <./Mapping_hu:Album_infobox/SimplePropertyMapping/21> ,
			       <./Mapping_hu:Album_infobox/SimplePropertyMapping/20> ,
			       <./Mapping_hu:Album_infobox/SimplePropertyMapping/12> ,
			       <./Mapping_hu:Album_infobox/SimplePropertyMapping/16> ,
			       <./Mapping_hu:Album_infobox/SimplePropertyMapping/13> ,
			       <./Mapping_hu:Album_infobox/SimplePropertyMapping/17> ,
			       <./Mapping_hu:Album_infobox/ConditionalMapping/t%C3%ADpus_equals_Dal> ,
			       <./Mapping_hu:Album_infobox/SimplePropertyMapping/14> ,
			       <./Mapping_hu:Album_infobox/SimplePropertyMapping/18> ;
        rr:subjectMap          <./Mapping_hu:Album_infobox/SubjectMap> .


### Main SubjectMap
####################
<./Mapping_hu:Album_infobox/SubjectMap>
        a            rr:SubjectMap ;
        rr:template  "http://en.dbpedia.org/resource/{wikititle}" ;
        rr:termType  rr:IRI .


### LogicalSource
##################

<./Mapping_hu:Album_infobox/LogicalSource>
        a                         rml:LogicalSource ;
        rml:iterator              "Infobox" ;
        rml:referenceFormulation  ql:wikitext ;
        rml:source                "http://hu.wikipedia.org/wiki/Mapping_hu:Album_infobox?oldid=9351" .


