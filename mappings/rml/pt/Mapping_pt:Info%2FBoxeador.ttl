### Main TriplesMap
####################
<>      a                      rr:TriplesMap ;
        rml:logicalSource      <Boxeador/LogicalSource> ;
        rr:predicateObjectMap  <Boxeador/SimplePropertyMapping/6> ,
			       <Boxeador/SimplePropertyMapping/7> ,
			       <Boxeador/SimplePropertyMapping/0> ,
			       <Boxeador/SimplePropertyMapping/1> ,
			       <Boxeador/SimplePropertyMapping/3> ,
			       <Boxeador/SimplePropertyMapping/2> ,
			       <Boxeador/SimplePropertyMapping/4> ,
			       <Boxeador/SimplePropertyMapping/5> ;
        rr:subjectMap          <Boxeador/SubjectMap> .


### Main SubjectMap
####################
<Boxeador/SubjectMap>
        a            rr:SubjectMap ;
        rr:class     wikidata:Q5 ,
		     foaf:Person ,
		     dbo:Agent ,
		     schema:Person ,
		     owl:Thing ,
		     dbo:Athlete ,
		     dul:Agent ,
		     wikidata:Q215627 ,
		     dbo:Person ,
		     dul:NaturalPerson ,
		     dbo:Boxer ;
        rr:template  "http://en.dbpedia.org/resource/{wikititle}" ;
        rr:termType  rr:IRI .


### LogicalSource
##################

<Boxeador/LogicalSource>
        a                         rml:LogicalSource ;
        rml:iterator              "Infobox" ;
        rml:referenceFormulation  ql:wikitext ;
        rml:source                "http://pt.wikipedia.org/wiki/Mapping_pt:Info/Boxeador?oldid=13006" .


