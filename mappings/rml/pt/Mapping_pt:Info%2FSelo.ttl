### Main TriplesMap
####################
<>      a                      rr:TriplesMap ;
        rml:logicalSource      <Selo/LogicalSource> ;
        rr:predicateObjectMap  <Selo/SimplePropertyMapping/7> ,
			       <Selo/SimplePropertyMapping/2> ,
			       <Selo/SimplePropertyMapping/9> ,
			       <Selo/SimplePropertyMapping/1> ,
			       <Selo/SimplePropertyMapping/6> ,
			       <Selo/SimplePropertyMapping/8> ,
			       <Selo/SimplePropertyMapping/5> ,
			       <Selo/SimplePropertyMapping/0> ,
			       <Selo/SimplePropertyMapping/3> ,
			       <Selo/SimplePropertyMapping/4> ;
        rr:subjectMap          <Selo/SubjectMap> .


### Main SubjectMap
####################
<Selo/SubjectMap>  a  rr:SubjectMap ;
        rr:class     dul:Agent ,
		     schema:Organization ,
		     dbo:Agent ,
		     dbo:Organisation ,
		     dbo:RecordLabel ,
		     wikidata:Q18127 ,
		     owl:Thing ,
		     wikidata:Q43229 ,
		     dbo:Company ,
		     dul:SocialPerson ;
        rr:template  "http://en.dbpedia.org/resource/{wikititle}" ;
        rr:termType  rr:IRI .


### LogicalSource
##################

<Selo/LogicalSource>  a           rml:LogicalSource ;
        rml:iterator              "Infobox" ;
        rml:referenceFormulation  ql:wikitext ;
        rml:source                "http://pt.wikipedia.org/wiki/Mapping_pt:Info/Selo?oldid=13544" .


