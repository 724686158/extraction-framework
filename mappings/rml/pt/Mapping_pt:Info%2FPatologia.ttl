### Main TriplesMap
####################
<>      a                      rr:TriplesMap ;
        rml:logicalSource      <Patologia/LogicalSource> ;
        rr:predicateObjectMap  <Patologia/SimplePropertyMapping/9> ,
			       <Patologia/SimplePropertyMapping/3> ,
			       <Patologia/SimplePropertyMapping/6> ,
			       <Patologia/SimplePropertyMapping/0> ,
			       <Patologia/SimplePropertyMapping/2> ,
			       <Patologia/SimplePropertyMapping/5> ,
			       <Patologia/SimplePropertyMapping/8> ,
			       <Patologia/SimplePropertyMapping/1> ,
			       <Patologia/SimplePropertyMapping/4> ,
			       <Patologia/SimplePropertyMapping/7> ;
        rr:subjectMap          <Patologia/SubjectMap> .


### Main SubjectMap
####################
<Patologia/SubjectMap>
        a            rr:SubjectMap ;
        rr:class     dbo:Disease ,
		     wikidata:Q12136 ,
		     owl:Thing ;
        rr:template  "http://en.dbpedia.org/resource/{wikititle}" ;
        rr:termType  rr:IRI .


### LogicalSource
##################

<Patologia/LogicalSource>
        a                         rml:LogicalSource ;
        rml:iterator              "Infobox" ;
        rml:referenceFormulation  ql:wikitext ;
        rml:source                "http://pt.wikipedia.org/wiki/Mapping_pt:Info/Patologia?oldid=17518" .


