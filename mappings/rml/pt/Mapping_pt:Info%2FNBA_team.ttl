			       <NBA_team/SimplePropertyMapping/2> ,
			       <NBA_team/SimplePropertyMapping/5> ,
			       <NBA_team/SimplePropertyMapping/4> ;
        rr:subjectMap          <NBA_team/SubjectMap> .


### Main SubjectMap
####################
<NBA_team/SubjectMap>
        a            rr:SubjectMap ;
        rr:class     wikidata:Q43229 ,
		     schema:SportsTeam ,
		     dbo:SportsTeam ,
		     dbo:Agent ,
		     schema:Organization ,
		     dul:SocialPerson ,
		     dbo:Organisation ,
		     owl:Thing ,
		     dul:Agent ,
		     dbo:BasketballTeam ;
        rr:template  "http://en.dbpedia.org/resource/{wikititle}" ;
        rr:termType  rr:IRI .


### LogicalSource
##################

<NBA_team/LogicalSource>
        a                         rml:LogicalSource ;
        rml:iterator              "Infobox" ;
        rml:referenceFormulation  ql:wikitext ;
        rml:source                "http://pt.wikipedia.org/wiki/Mapping_pt:Info/NBA_team?oldid=13145" .


