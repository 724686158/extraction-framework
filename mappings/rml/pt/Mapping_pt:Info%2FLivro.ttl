        rml:logicalSource      <Livro/LogicalSource> ;
        rr:predicateObjectMap  <Livro/Function/SimplePropertyFunction> ,
			       <Livro/SimplePropertyMapping/2/FTM/FV/propertyParameterPOM> ,
			       <Livro/SimplePropertyMapping/2/FTM/FV/ontologyPropertyParameterPOM> ;
        rr:subjectMap          <Livro/SubjectMap/Function> .

