			       <Bandeira/SimplePropertyMapping/11> ,
			       <Bandeira/SimplePropertyMapping/3> ,
			       <Bandeira/SimplePropertyMapping/8> ;
        rr:subjectMap          <Bandeira/SubjectMap> .


### Main SubjectMap
####################
<Bandeira/SubjectMap>
        a            rr:SubjectMap ;
        rr:class     dbo:Flag ,
		     wikidata:Q14660 ,
		     owl:Thing ;
        rr:template  "http://en.dbpedia.org/resource/{wikititle}" ;
        rr:termType  rr:IRI .


### LogicalSource
##################

<Bandeira/LogicalSource>
        a                         rml:LogicalSource ;
        rml:iterator              "Infobox" ;
        rml:referenceFormulation  ql:wikitext ;
        rml:source                "http://pt.wikipedia.org/wiki/Mapping_pt:Info/Bandeira?oldid=16992" .


