### Main TriplesMap
####################
<>      a                      rr:TriplesMap ;
        rml:logicalSource      <Bebida/LogicalSource> ;
        rr:predicateObjectMap  <Bebida/SimplePropertyMapping/8> ,
			       <Bebida/SimplePropertyMapping/5> ,
			       <Bebida/SimplePropertyMapping/12> ,
			       <Bebida/SimplePropertyMapping/2> ,
			       <Bebida/SimplePropertyMapping/7> ,
			       <Bebida/SimplePropertyMapping/4> ,
			       <Bebida/SimplePropertyMapping/1> ,
			       <Bebida/SimplePropertyMapping/11> ,
			       <Bebida/SimplePropertyMapping/9> ,
			       <Bebida/SimplePropertyMapping/3> ,
			       <Bebida/SimplePropertyMapping/6> ,
			       <Bebida/SimplePropertyMapping/0> ,
			       <Bebida/SimplePropertyMapping/10> ;
        rr:subjectMap          <Bebida/SubjectMap> .


### Main SubjectMap
####################
<Bebida/SubjectMap>  a  rr:SubjectMap ;
        rr:class     dbo:Beverage ,
		     dbo:Food ,
		     wikidata:Q2095 ,
		     dul:FunctionalSubstance ,
		     owl:Thing ;
        rr:template  "http://en.dbpedia.org/resource/{wikititle}" ;
        rr:termType  rr:IRI .


### LogicalSource
##################

<Bebida/LogicalSource>
        a                         rml:LogicalSource ;
        rml:iterator              "Infobox" ;
        rml:referenceFormulation  ql:wikitext ;
        rml:source                "http://pt.wikipedia.org/wiki/Mapping_pt:Info/Bebida?oldid=17151" .


