### Main TriplesMap
####################
<>      a                      rr:TriplesMap ;
        dcterms:type           "templateMapping" ;
        rml:logicalSource      <./Mapping_en:Infobox_comics_creator/LogicalSource> ;
        rr:predicateObjectMap  <./Mapping_en:Infobox_comics_creator/SimplePropertyMapping/foaf:nick/alias> ,
			       <./Mapping_en:Infobox_comics_creator/SimplePropertyMapping/foaf:homepage/website> ,
			       <./Mapping_en:Infobox_comics_creator/SimplePropertyMapping/deathDate/deathdate> ,
			       <./Mapping_en:Infobox_comics_creator/SimplePropertyMapping/birthDate/birthdate> ,
			       <./Mapping_en:Infobox_comics_creator/SimplePropertyMapping/birthPlace/location> ,
			       <./Mapping_en:Infobox_comics_creator/SimplePropertyMapping/award/awards> ,
			       <./Mapping_en:Infobox_comics_creator/SimplePropertyMapping/foaf:name/birthname> ,
			       <./Mapping_en:Infobox_comics_creator/SimplePropertyMapping/foaf:name/name> ,
			       <./Mapping_en:Infobox_comics_creator/SimplePropertyMapping/nationality/nationality> ,
			       <./Mapping_en:Infobox_comics_creator/SimplePropertyMapping/deathPlace/deathplace> ;
        rr:subjectMap          <./Mapping_en:Infobox_comics_creator/SubjectMap> .


### Main SubjectMap
####################
<./Mapping_en:Infobox_comics_creator/SubjectMap>
        a            rr:SubjectMap ;
        rr:class     dbo:Agent ,
		     dbo:Person ,
		     owl:Thing ,
		     wikidata:Q5 ,
		     wikidata:Q483501 ,
		     foaf:Person ,
		     schema:Person ,
		     dbo:ComicsCreator ,
		     wikidata:Q215627 ,
		     dul:Agent ,
		     dul:NaturalPerson ,
		     dbo:Artist ;
        rr:template  "http://en.dbpedia.org/resource/{wikititle}" ;
        rr:termType  rr:IRI .


