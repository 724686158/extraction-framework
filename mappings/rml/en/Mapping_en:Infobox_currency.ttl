### Main TriplesMap
####################
<>      a                      rr:TriplesMap ;
        dcterms:type           "templateMapping" ;
        rml:logicalSource      <./Mapping_en:Infobox_currency/LogicalSource> ;
        rr:predicateObjectMap  <./Mapping_en:Infobox_currency/SimplePropertyMapping/usingCountry/using_countries> ,
			       <./Mapping_en:Infobox_currency/SimplePropertyMapping/foaf:name/currency_name_in_local> ;
        rr:subjectMap          <./Mapping_en:Infobox_currency/SubjectMap> .


### Main SubjectMap
####################
<./Mapping_en:Infobox_currency/SubjectMap>
        a            rr:SubjectMap ;
        rr:class     dbo:Currency ,
		     owl:Thing ;
        rr:template  "http://en.dbpedia.org/resource/{wikititle}" ;
        rr:termType  rr:IRI .


### LogicalSource
##################

<./Mapping_en:Infobox_currency/LogicalSource>
        a                         rml:LogicalSource ;
        rml:iterator              "Infobox" ;
        rml:referenceFormulation  ql:wikitext ;
        rml:source                "http://en.wikipedia.org/wiki/Mapping_en:Infobox_currency?oldid=7289" .


###########
# Mappings
###########

### PredicateObjectMap
#######################
<./Mapping_en:Infobox_currency/SimplePropertyMapping/foaf:name/currency_name_in_local>
        a             rr:PredicateObjectMap ;
        dcterms:type  "simplePropertyMapping" ;
        rr:objectMap  <./Mapping_en:Infobox_currency/SimplePropertyMapping/foaf:name/currency_name_in_local/FunctionTermMap> ;
        rr:predicate  foaf:name .

### Function Term Map
<./Mapping_en:Infobox_currency/SimplePropertyMapping/foaf:name/currency_name_in_local/FunctionTermMap>
        a                   fnml:FunctionTermMap ;
        fnml:functionValue  <./Mapping_en:Infobox_currency/SimplePropertyMapping/foaf:name/currency_name_in_local/FunctionTermMap/FunctionValue> .

### Function Value
<./Mapping_en:Infobox_currency/SimplePropertyMapping/foaf:name/currency_name_in_local/FunctionTermMap/FunctionValue>
        a                      rr:TriplesMap ;
        rml:logicalSource      <./Mapping_en:Infobox_currency/LogicalSource> ;
        rr:predicateObjectMap  <./Mapping_en:Infobox_currency/Function/SimplePropertyFunction> ,
			       <./Mapping_en:Infobox_currency/SimplePropertyMapping/foaf:name/currency_name_in_local/FunctionTermMap/FunctionValue/propertyParameterPOM> ,
			       <./Mapping_en:Infobox_currency/SimplePropertyMapping/foaf:name/currency_name_in_local/FunctionTermMap/FunctionValue/ontologyPropertyParameterPOM> ;
        rr:subjectMap          <./Mapping_en:Infobox_currency/SubjectMap/Function> .

### Parameters
<./Mapping_en:Infobox_currency/SimplePropertyMapping/foaf:name/currency_name_in_local/FunctionTermMap/FunctionValue/propertyParameterPOM>
        a             rr:PredicateObjectMap ;
        rr:objectMap  <./Mapping_en:Infobox_currency/SimplePropertyMapping/foaf:name/currency_name_in_local/FunctionTermMap/FunctionValue/propertyParameterPOM/ObjectMap> ;
        rr:predicate  dbf:propertyParameter .

<./Mapping_en:Infobox_currency/SimplePropertyMapping/foaf:name/currency_name_in_local/FunctionTermMap/FunctionValue/ontologyPropertyParameterPOM>
        a             rr:PredicateObjectMap ;
        rr:object     "foaf:name" ;
        rr:predicate  dbf:ontologyPropertyParameter .

### References
<./Mapping_en:Infobox_currency/SimplePropertyMapping/foaf:name/currency_name_in_local/FunctionTermMap/FunctionValue/propertyParameterPOM/ObjectMap>
        a              rr:ObjectMap ;
        rml:reference  "currency_name_in_local" .


### PredicateObjectMap
#######################
<./Mapping_en:Infobox_currency/SimplePropertyMapping/usingCountry/using_countries>
        a             rr:PredicateObjectMap ;
        dcterms:type  "simplePropertyMapping" ;
        rr:objectMap  <./Mapping_en:Infobox_currency/SimplePropertyMapping/usingCountry/using_countries/FunctionTermMap> ;
        rr:predicate  dbo:usingCountry .

### Function Term Map
<./Mapping_en:Infobox_currency/SimplePropertyMapping/usingCountry/using_countries/FunctionTermMap>
        a                   fnml:FunctionTermMap ;
        fnml:functionValue  <./Mapping_en:Infobox_currency/SimplePropertyMapping/usingCountry/using_countries/FunctionTermMap/FunctionValue> .

### Function Value
<./Mapping_en:Infobox_currency/SimplePropertyMapping/usingCountry/using_countries/FunctionTermMap/FunctionValue>
        a                      rr:TriplesMap ;
        rml:logicalSource      <./Mapping_en:Infobox_currency/LogicalSource> ;
        rr:predicateObjectMap  <./Mapping_en:Infobox_currency/Function/SimplePropertyFunction> ,
			       <./Mapping_en:Infobox_currency/SimplePropertyMapping/usingCountry/using_countries/FunctionTermMap/FunctionValue/propertyParameterPOM> ,
			       <./Mapping_en:Infobox_currency/SimplePropertyMapping/usingCountry/using_countries/FunctionTermMap/FunctionValue/ontologyPropertyParameterPOM> ;
        rr:subjectMap          <./Mapping_en:Infobox_currency/SubjectMap/Function> .

### Parameters
<./Mapping_en:Infobox_currency/SimplePropertyMapping/usingCountry/using_countries/FunctionTermMap/FunctionValue/propertyParameterPOM>
        a             rr:PredicateObjectMap ;
        rr:objectMap  <./Mapping_en:Infobox_currency/SimplePropertyMapping/usingCountry/using_countries/FunctionTermMap/FunctionValue/propertyParameterPOM/ObjectMap> ;
        rr:predicate  dbf:propertyParameter .

<./Mapping_en:Infobox_currency/SimplePropertyMapping/usingCountry/using_countries/FunctionTermMap/FunctionValue/ontologyPropertyParameterPOM>
        a             rr:PredicateObjectMap ;
