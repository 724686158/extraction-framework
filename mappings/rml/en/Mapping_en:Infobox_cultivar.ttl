### Main TriplesMap
####################
<>      a                      rr:TriplesMap ;
        dcterms:type           "templateMapping" ;
        rml:logicalSource      <./Mapping_en:Infobox_cultivar/LogicalSource> ;
        rr:predicateObjectMap  <./Mapping_en:Infobox_cultivar/SimplePropertyMapping/species/species> ,
			       <./Mapping_en:Infobox_cultivar/SimplePropertyMapping/hybrid/hybrid> ,
			       <./Mapping_en:Infobox_cultivar/SimplePropertyMapping/foaf:name/name> ,
			       <./Mapping_en:Infobox_cultivar/SimplePropertyMapping/cultivatedVariety/cultivar> ,
			       <./Mapping_en:Infobox_cultivar/SimplePropertyMapping/origin/origin> ,
			       <./Mapping_en:Infobox_cultivar/SimplePropertyMapping/foaf:name/marketing_names> ;
        rr:subjectMap          <./Mapping_en:Infobox_cultivar/SubjectMap> .


### Main SubjectMap
####################
<./Mapping_en:Infobox_cultivar/SubjectMap>
        a            rr:SubjectMap ;
        rr:class     dbo:Plant ,
		     dbo:Species ,
		     dbo:Eukaryote ,
		     owl:Thing ,
		     wikidata:Q756 ,
		     dbo:CultivatedVariety ,
		     wikidata:Q4886 ,
		     wikidata:Q19088 ;
        rr:template  "http://en.dbpedia.org/resource/{wikititle}" ;
        rr:termType  rr:IRI .


### LogicalSource
##################

<./Mapping_en:Infobox_cultivar/LogicalSource>
        a                         rml:LogicalSource ;
        rml:iterator              "Infobox" ;
        rml:referenceFormulation  ql:wikitext ;
        rml:source                "http://en.wikipedia.org/wiki/Mapping_en:Infobox_cultivar?oldid=21293" .


