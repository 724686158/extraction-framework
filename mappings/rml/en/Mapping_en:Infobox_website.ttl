### Main TriplesMap
####################
<>      a                      rr:TriplesMap ;
        rml:logicalSource      <./Mapping_en:Infobox_website/LogicalSource> ;
        rr:predicateObjectMap  <./Mapping_en:Infobox_website/SimplePropertyMapping/owner/owner> ,
			       <./Mapping_en:Infobox_website/SimplePropertyMapping/foaf:name/name> ,
			       <./Mapping_en:Infobox_website/SimplePropertyMapping/type/type> ,
			       <./Mapping_en:Infobox_website/SimplePropertyMapping/language/language> ,
			       <./Mapping_en:Infobox_website/SimplePropertyMapping/author/author> ,
			       <./Mapping_en:Infobox_website/SimplePropertyMapping/foaf:homepage/url> ;
        rr:subjectMap          <./Mapping_en:Infobox_website/SubjectMap> .


### Main SubjectMap
####################
<./Mapping_en:Infobox_website/SubjectMap>
        a            rr:SubjectMap ;
        rr:class     dbo:Website ,
		     schema:WebPage ,
		     dbo:Work ,
		     schema:CreativeWork ,
		     wikidata:Q386724 ,
		     owl:Thing ;
        rr:template  "http://en.dbpedia.org/resource/{wikititle}" ;
        rr:termType  rr:IRI .


### LogicalSource
##################

<./Mapping_en:Infobox_website/LogicalSource>
        a                         rml:LogicalSource ;
        rml:iterator              "Infobox" ;
        rml:referenceFormulation  ql:wikitext ;
        rml:source                "http://en.wikipedia.org/wiki/Mapping_en:Infobox_website?oldid=28637" .


