			       <./Mapping_en:Infobox_publisher/SimplePropertyMapping/7> ,
			       <./Mapping_en:Infobox_publisher/SimplePropertyMapping/4> ;
        rr:subjectMap          <./Mapping_en:Infobox_publisher/SubjectMap> .


### Main SubjectMap
####################
<./Mapping_en:Infobox_publisher/SubjectMap>
        a            rr:SubjectMap ;
        rr:class     dbo:Agent ,
		     dul:SocialPerson ,
		     wikidata:Q43229 ,
		     owl:Thing ,
		     dbo:Publisher ,
		     dbo:Organisation ,
		     schema:Organization ,
		     dul:Agent ,
		     dbo:Company ;
        rr:template  "http://en.dbpedia.org/resource/{wikititle}" ;
        rr:termType  rr:IRI .


### LogicalSource
##################

<./Mapping_en:Infobox_publisher/LogicalSource>
        a                         rml:LogicalSource ;
        rml:iterator              "Infobox" ;
        rml:referenceFormulation  ql:wikitext ;
        rml:source                "http://en.wikipedia.org/wiki/Mapping_en:Infobox_publisher?oldid=20790" .


