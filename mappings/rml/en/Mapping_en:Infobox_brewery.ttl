### Main TriplesMap
####################
<>      a                      rr:TriplesMap ;
        rml:logicalSource      <./Mapping_en:Infobox_brewery/LogicalSource> ;
        rr:predicateObjectMap  <./Mapping_en:Infobox_brewery/SimplePropertyMapping/location/location> ,
			       <./Mapping_en:Infobox_brewery/SimplePropertyMapping/foaf:name/name> ,
			       <./Mapping_en:Infobox_brewery/SimplePropertyMapping/owner/owner> ,
			       <./Mapping_en:Infobox_brewery/SimplePropertyMapping/foaf:homepage/url> ,
			       <./Mapping_en:Infobox_brewery/latitude> ,
			       <./Mapping_en:Infobox_brewery/longitude> ;
        rr:subjectMap          <./Mapping_en:Infobox_brewery/SubjectMap> .


### Main SubjectMap
####################
<./Mapping_en:Infobox_brewery/SubjectMap>
        a            rr:SubjectMap ;
        rr:class     schema:Organization ,
		     dbo:Brewery ,
		     dbo:Company ,
		     dul:SocialPerson ,
		     wikidata:Q43229 ,
		     dbo:Organisation ,
		     dbo:Agent ,
		     owl:Thing ,
		     dul:Agent ;
        rr:template  "http://en.dbpedia.org/resource/{wikititle}" ;
        rr:termType  rr:IRI .


### LogicalSource
##################

<./Mapping_en:Infobox_brewery/LogicalSource>
        a                         rml:LogicalSource ;
        rml:iterator              "Infobox" ;
        rml:referenceFormulation  ql:wikitext ;
        rml:source                "http://en.wikipedia.org/wiki/Mapping_en:Infobox_brewery?oldid=29724" .


