### Main TriplesMap
####################
<>      a                      rr:TriplesMap ;
        rml:logicalSource      <./Mapping_en:Infobox_enzyme/LogicalSource> ;
        rr:predicateObjectMap  <./Mapping_en:Infobox_enzyme/SimplePropertyMapping/0> ,
			       <./Mapping_en:Infobox_enzyme/SimplePropertyMapping/1> ;
        rr:subjectMap          <./Mapping_en:Infobox_enzyme/SubjectMap> .


### Main SubjectMap
####################
<./Mapping_en:Infobox_enzyme/SubjectMap>
        a            rr:SubjectMap ;
        rr:class     dbo:Enzyme ,
		     wikidata:Q8047 ,
		     dbo:Biomolecule ,
		     wikidata:Q206229 ,
		     owl:Thing ;
        rr:template  "http://en.dbpedia.org/resource/{wikititle}" ;
        rr:termType  rr:IRI .


### LogicalSource
##################

<./Mapping_en:Infobox_enzyme/LogicalSource>
        a                         rml:LogicalSource ;
        rml:iterator              "Infobox" ;
        rml:referenceFormulation  ql:wikitext ;
        rml:source                "http://en.wikipedia.org/wiki/Mapping_en:Infobox_enzyme?oldid=20503" .


###########
# Mappings
###########

### Predicate Object Map
#########################
<./Mapping_en:Infobox_enzyme/SimplePropertyMapping/1>
        a             rr:PredicateObjectMap ;
        rr:objectMap  <./Mapping_en:Infobox_enzyme/SimplePropertyMapping/1/FTM> ;
        rr:predicate  dbo:ecNumber .

### Function Term Map
<./Mapping_en:Infobox_enzyme/SimplePropertyMapping/1/FTM>
        a                   fnml:FunctionTermMap ;
        fnml:functionValue  <./Mapping_en:Infobox_enzyme/SimplePropertyMapping/1/FTM/FV> .

### Function Execution Mapping
<./Mapping_en:Infobox_enzyme/SimplePropertyMapping/1/FTM/FV>
        a                      rr:TriplesMap ;
        rml:logicalSource      <./Mapping_en:Infobox_enzyme/LogicalSource> ;
        rr:predicateObjectMap  <./Mapping_en:Infobox_enzyme/Function/SimplePropertyFunction> ,
			       <./Mapping_en:Infobox_enzyme/SimplePropertyMapping/1/FTM/FV/propertyParameterPOM> ,
			       <./Mapping_en:Infobox_enzyme/SimplePropertyMapping/1/FTM/FV/ontologyPropertyParameterPOM> ;
        rr:subjectMap          <./Mapping_en:Infobox_enzyme/SubjectMap/Function> .

### Parameters
<./Mapping_en:Infobox_enzyme/SimplePropertyMapping/1/FTM/FV/propertyParameterPOM>
        a             rr:PredicateObjectMap ;
        rr:objectMap  <./Mapping_en:Infobox_enzyme/SimplePropertyMapping/1/FTM/FV/propertyParameterPOM/ObjectMap> ;
        rr:predicate  dbf:propertyParameter .

<./Mapping_en:Infobox_enzyme/SimplePropertyMapping/1/FTM/FV/ontologyPropertyParameterPOM>
        a             rr:PredicateObjectMap ;
        rr:object     "ecNumber" ;
        rr:predicate  dbf:ontologyPropertyParameter .

### References
<./Mapping_en:Infobox_enzyme/SimplePropertyMapping/1/FTM/FV/propertyParameterPOM/ObjectMap>
        a              rr:ObjectMap ;
        rml:reference  "EC_number" .


### Predicate Object Map
#########################
<./Mapping_en:Infobox_enzyme/SimplePropertyMapping/0>
        a             rr:PredicateObjectMap ;
        rr:objectMap  <./Mapping_en:Infobox_enzyme/SimplePropertyMapping/0/FTM> ;
        rr:predicate  foaf:name .

### Function Term Map
<./Mapping_en:Infobox_enzyme/SimplePropertyMapping/0/FTM>
        a                   fnml:FunctionTermMap ;
        fnml:functionValue  <./Mapping_en:Infobox_enzyme/SimplePropertyMapping/0/FTM/FV> .

### Function Execution Mapping
<./Mapping_en:Infobox_enzyme/SimplePropertyMapping/0/FTM/FV>
        a                      rr:TriplesMap ;
        rml:logicalSource      <./Mapping_en:Infobox_enzyme/LogicalSource> ;
        rr:predicateObjectMap  <./Mapping_en:Infobox_enzyme/Function/SimplePropertyFunction> ,
			       <./Mapping_en:Infobox_enzyme/SimplePropertyMapping/0/FTM/FV/propertyParameterPOM> ,
			       <./Mapping_en:Infobox_enzyme/SimplePropertyMapping/0/FTM/FV/ontologyPropertyParameterPOM> ;
        rr:subjectMap          <./Mapping_en:Infobox_enzyme/SubjectMap/Function> .

### Parameters
<./Mapping_en:Infobox_enzyme/SimplePropertyMapping/0/FTM/FV/ontologyPropertyParameterPOM>
        a             rr:PredicateObjectMap ;
        rr:object     "foaf:name" ;
        rr:predicate  dbf:ontologyPropertyParameter .

<./Mapping_en:Infobox_enzyme/SimplePropertyMapping/0/FTM/FV/propertyParameterPOM>
        a             rr:PredicateObjectMap ;
        rr:objectMap  <./Mapping_en:Infobox_enzyme/SimplePropertyMapping/0/FTM/FV/propertyParameterPOM/ObjectMap> ;
        rr:predicate  dbf:propertyParameter .

### References
<./Mapping_en:Infobox_enzyme/SimplePropertyMapping/0/FTM/FV/propertyParameterPOM/ObjectMap>
        a              rr:ObjectMap ;
        rml:reference  "Name" .


######################
# Conditional Mappings
######################



############
# Functions
############


<./Mapping_en:Infobox_enzyme/Function/SimplePropertyFunction>
        a             rr:PredicateObjectMap ;
        rr:object     dbf:simplePropertyFunction ;
        rr:predicate  fno:executes .


### Functions SubjectMap
#########################

<./Mapping_en:Infobox_enzyme/SubjectMap/Function>
        a            rr:SubjectMap ;
        rr:class     fno:Execution ;
        rr:termType  rr:BlankNode .
