### Main TriplesMap
####################
<>      a                      rr:TriplesMap ;
        dcterms:type           "conditionalMapping" ;
        rml:logicalSource      <./Mapping_en:Infobox_horseracing_personality/LogicalSource> ;
        rr:predicateObjectMap  <./Mapping_en:Infobox_horseracing_personality/SimplePropertyMapping/race/race> ,
			       <./Mapping_en:Infobox_horseracing_personality/SimplePropertyMapping/deathPlace/death_place> ,
			       <./Mapping_en:Infobox_horseracing_personality/SimplePropertyMapping/birthPlace/birth_place> ,
			       <./Mapping_en:Infobox_horseracing_personality/SimplePropertyMapping/raceHorse/horses> ,
			       <./Mapping_en:Infobox_horseracing_personality/ConditionalMapping/occupation_equals_Jockey> ,
			       <./Mapping_en:Infobox_horseracing_personality/SimplePropertyMapping/honours/honours> ,
			       <./Mapping_en:Infobox_horseracing_personality/SimplePropertyMapping/birthDate/birth_date> ,
			       <./Mapping_en:Infobox_horseracing_personality/SimplePropertyMapping/deathDate/death_date> ,
			       <./Mapping_en:Infobox_horseracing_personality/SimplePropertyMapping/foaf:name/name> ,
			       <./Mapping_en:Infobox_horseracing_personality/SimplePropertyMapping/wins/career%20wins> ,
			       <./Mapping_en:Infobox_horseracing_personality/SimplePropertyMapping/award/awards> ,
			       <./Mapping_en:Infobox_horseracing_personality/SimplePropertyMapping/occupation/occupation> ;
        rr:subjectMap          <./Mapping_en:Infobox_horseracing_personality/SubjectMap> .


### Main SubjectMap
####################
<./Mapping_en:Infobox_horseracing_personality/SubjectMap>
        a            rr:SubjectMap ;
        rr:template  "http://en.dbpedia.org/resource/{wikititle}" ;
        rr:termType  rr:IRI .


