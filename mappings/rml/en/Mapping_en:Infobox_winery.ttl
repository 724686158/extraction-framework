### Main TriplesMap
####################
<>      a                      rr:TriplesMap ;
        rml:logicalSource      <./Mapping_en:Infobox_winery/LogicalSource> ;
        rr:predicateObjectMap  <./Mapping_en:Infobox_winery/SimplePropertyMapping/foaf:name/winery_name> ,
			       <./Mapping_en:Infobox_winery/SimplePropertyMapping/location/location_country> ,
			       <./Mapping_en:Infobox_winery/SimplePropertyMapping/location/location_city> ,
			       <./Mapping_en:Infobox_winery/SimplePropertyMapping/foaf:homepage/homepage> ;
        rr:subjectMap          <./Mapping_en:Infobox_winery/SubjectMap> .


### Main SubjectMap
####################
<./Mapping_en:Infobox_winery/SubjectMap>
        a            rr:SubjectMap ;
        rr:class     wikidata:Q43229 ,
		     dbo:Agent ,
		     dbo:Winery ,
		     dbo:Company ,
		     dul:Agent ,
		     schema:Organization ,
		     owl:Thing ,
		     dul:SocialPerson ,
		     wikidata:Q156362 ,
		     dbo:Organisation ;
        rr:template  "http://en.dbpedia.org/resource/{wikititle}" ;
        rr:termType  rr:IRI .


### LogicalSource
##################

<./Mapping_en:Infobox_winery/LogicalSource>
        a                         rml:LogicalSource ;
        rml:iterator              "Infobox" ;
        rml:referenceFormulation  ql:wikitext ;
        rml:source                "http://en.wikipedia.org/wiki/Mapping_en:Infobox_winery?oldid=29737" .


