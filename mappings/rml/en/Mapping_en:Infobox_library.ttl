### Main TriplesMap
####################
<>      a                      rr:TriplesMap ;
        dcterms:type           "templateMapping" ;
        rml:logicalSource      <./Mapping_en:Infobox_library/LogicalSource> ;
        rr:predicateObjectMap  <./Mapping_en:Infobox_library/SimplePropertyMapping/foaf:name/library_name> ,
			       <./Mapping_en:Infobox_library/SimplePropertyMapping/foaf:name/name_en> ,
			       <./Mapping_en:Infobox_library/SimplePropertyMapping/foaf:homepage/website> ,
			       <./Mapping_en:Infobox_library/latitude> ,
			       <./Mapping_en:Infobox_library/longitude> ,
			       <./Mapping_en:Infobox_library/SimplePropertyMapping/foundingDate/established> ,
			       <./Mapping_en:Infobox_library/SimplePropertyMapping/country/country> ;
        rr:subjectMap          <./Mapping_en:Infobox_library/SubjectMap> .


### Main SubjectMap
####################
<./Mapping_en:Infobox_library/SubjectMap>
        a            rr:SubjectMap ;
        rr:class     dul:Agent ,
		     schema:EducationalOrganization ,
		     schema:Library ,
		     dbo:ArchitecturalStructure ,
		     dbo:Location ,
		     dbo:EducationalInstitution ,
		     schema:Organization ,
		     wikidata:Q7075 ,
		     wikidata:Q43229 ,
		     dbo:Place ,
		     dul:SocialPerson ,
		     wikidata:Q41176 ,
		     dbo:Organisation ,
		     dbo:Library ,
		     dbo:Agent ,
		     owl:Thing ,
		     dbo:Building ,
		     schema:Place ;
        rr:template  "http://en.dbpedia.org/resource/{wikititle}" ;
        rr:termType  rr:IRI .


### LogicalSource
##################

<./Mapping_en:Infobox_library/LogicalSource>
        a                         rml:LogicalSource ;
        rml:iterator              "Infobox" ;
        rml:referenceFormulation  ql:wikitext ;
        rml:source                "http://en.wikipedia.org/wiki/Mapping_en:Infobox_library?oldid=10883" .


###########
# Mappings
###########

### PredicateObjectMap
#######################
<./Mapping_en:Infobox_library/SimplePropertyMapping/foaf:name/library_name>
        a             rr:PredicateObjectMap ;
        dcterms:type  "simplePropertyMapping" ;
        rr:objectMap  <./Mapping_en:Infobox_library/SimplePropertyMapping/foaf:name/library_name/FunctionTermMap> ;
        rr:predicate  foaf:name .

### Function Term Map
<./Mapping_en:Infobox_library/SimplePropertyMapping/foaf:name/library_name/FunctionTermMap>
        a                   fnml:FunctionTermMap ;
        fnml:functionValue  <./Mapping_en:Infobox_library/SimplePropertyMapping/foaf:name/library_name/FunctionTermMap/FunctionValue> .

### Function Value
<./Mapping_en:Infobox_library/SimplePropertyMapping/foaf:name/library_name/FunctionTermMap/FunctionValue>
        a                      rr:TriplesMap ;
        rml:logicalSource      <./Mapping_en:Infobox_library/LogicalSource> ;
        rr:predicateObjectMap  <./Mapping_en:Infobox_library/Function/SimplePropertyFunction> ,
			       <./Mapping_en:Infobox_library/SimplePropertyMapping/foaf:name/library_name/FunctionTermMap/FunctionValue/propertyParameterPOM> ,
			       <./Mapping_en:Infobox_library/SimplePropertyMapping/foaf:name/library_name/FunctionTermMap/FunctionValue/ontologyPropertyParameterPOM> ;
        rr:subjectMap          <./Mapping_en:Infobox_library/SubjectMap/Function> .

### Parameters
<./Mapping_en:Infobox_library/SimplePropertyMapping/foaf:name/library_name/FunctionTermMap/FunctionValue/ontologyPropertyParameterPOM>
        a             rr:PredicateObjectMap ;
        rr:object     "foaf:name" ;
        rr:predicate  dbf:ontologyPropertyParameter .

<./Mapping_en:Infobox_library/SimplePropertyMapping/foaf:name/library_name/FunctionTermMap/FunctionValue/propertyParameterPOM>
        a             rr:PredicateObjectMap ;
        rr:objectMap  <./Mapping_en:Infobox_library/SimplePropertyMapping/foaf:name/library_name/FunctionTermMap/FunctionValue/propertyParameterPOM/ObjectMap> ;
        rr:predicate  dbf:propertyParameter .

### References
<./Mapping_en:Infobox_library/SimplePropertyMapping/foaf:name/library_name/FunctionTermMap/FunctionValue/propertyParameterPOM/ObjectMap>
        a              rr:ObjectMap ;
        rml:reference  "library_name" .


