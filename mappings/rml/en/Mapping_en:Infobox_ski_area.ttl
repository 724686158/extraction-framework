        a             rr:SubjectMap ;
        rdfs:comment  "Main Subject Map of Mapping en:Infobox ski area. This defines the generation of the subject for the triples that are extracted from Infobox ski area." ;
        rdfs:label    "Mapping en:Infobox ski area (Subject Map)" ;
        rr:class      dbo:SkiArea ,
