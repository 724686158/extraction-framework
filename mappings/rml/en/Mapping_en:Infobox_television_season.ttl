        rr:predicateObjectMap  <./Mapping_en:Infobox_television_season/SimplePropertyMapping/country/country> ,
			       <./Mapping_en:Infobox_television_season/SimplePropertyMapping/foaf:name/season_name> ,
			       <./Mapping_en:Infobox_television_season/SimplePropertyMapping/network/network> ,
			       <./Mapping_en:Infobox_television_season/SimplePropertyMapping/firstAirDate/first_aired> ,
			       <./Mapping_en:Infobox_television_season/SimplePropertyMapping/numberOfEpisodes/num_episodes> ;
        rr:subjectMap          <./Mapping_en:Infobox_television_season/SubjectMap> .


### Main SubjectMap
####################
<./Mapping_en:Infobox_television_season/SubjectMap>
        a            rr:SubjectMap ;
        rr:class     dbo:TelevisionSeason ,
		     dbo:Work ,
		     schema:CreativeWork ,
		     wikidata:Q386724 ,
		     owl:Thing ;
        rr:template  "http://en.dbpedia.org/resource/{wikititle}" ;
        rr:termType  rr:IRI .


