### Main TriplesMap
####################
<>      a                      rr:TriplesMap ;
        rdfs:comment           "Main Triples Map of Mapping en:Infobox monument. This defines the generation of triples that are extracted from Infobox monument." ;
        rdfs:label             "Mapping en:Infobox monument (Triples Map)" ;
        rml:logicalSource      <./Mapping_en:Infobox_monument/LogicalSource> ;
        rr:predicateObjectMap  <./Mapping_en:Infobox_monument/SimplePropertyMapping/1> ,
			       <./Mapping_en:Infobox_monument/longitude> ,
			       <./Mapping_en:Infobox_monument/SimplePropertyMapping/2> ,
			       <./Mapping_en:Infobox_monument/SimplePropertyMapping/0> ,
			       <./Mapping_en:Infobox_monument/latitude> ;
        rr:subjectMap          <./Mapping_en:Infobox_monument/SubjectMap> .


### Main SubjectMap
####################
<./Mapping_en:Infobox_monument/SubjectMap>
        a             rr:SubjectMap ;
        rdfs:comment  "Main Subject Map of Mapping en:Infobox monument. This defines the generation of the subject for the triples that are extracted from Infobox monument." ;
        rdfs:label    "Mapping en:Infobox monument (Subject Map)" ;
        rr:class      owl:Thing ,
		     schema:Place ,
		     wikidata:Q4989906 ,
		     dbo:Place ,
		     dbo:Monument ,
		     dbo:Location ;
        rr:template   "http://en.dbpedia.org/resource/{wikititle}" ;
        rr:termType   rr:IRI .


### LogicalSource
##################

<./Mapping_en:Infobox_monument/LogicalSource>
        a                         rml:LogicalSource ;
        rml:iterator              "Infobox" ;
        rml:referenceFormulation  ql:wikitext ;
        rml:source                "http://en.wikipedia.org/wiki/Mapping_en:Infobox_monument?oldid=51058" .


