### Main TriplesMap
####################
<>      a                      rr:TriplesMap ;
        dcterms:type           "templateMapping" ;
        rml:logicalSource      <./Mapping_en:Infobox_poker_player/LogicalSource> ;
        rr:predicateObjectMap  <./Mapping_en:Infobox_poker_player/SimplePropertyMapping/hometown/hometown> ,
			       <./Mapping_en:Infobox_poker_player/SimplePropertyMapping/foaf:givenName/firstname> ,
			       <./Mapping_en:Infobox_poker_player/SimplePropertyMapping/foaf:familyName/lastname> ,
			       <./Mapping_en:Infobox_poker_player/SimplePropertyMapping/foaf:nick/nickname> ;
        rr:subjectMap          <./Mapping_en:Infobox_poker_player/SubjectMap> .


### Main SubjectMap
####################
<./Mapping_en:Infobox_poker_player/SubjectMap>
        a            rr:SubjectMap ;
        rr:class     dul:Agent ,
		     owl:Thing ,
		     dul:NaturalPerson ,
		     dbo:Person ,
		     foaf:Person ,
		     dbo:Agent ,
		     wikidata:Q215627 ,
		     dbo:PokerPlayer ,
		     schema:Person ,
		     dbo:Athlete ,
		     wikidata:Q5 ;
        rr:template  "http://en.dbpedia.org/resource/{wikititle}" ;
        rr:termType  rr:IRI .


### LogicalSource
##################

<./Mapping_en:Infobox_poker_player/LogicalSource>
        a                         rml:LogicalSource ;
        rml:iterator              "Infobox" ;
        rml:referenceFormulation  ql:wikitext ;
        rml:source                "http://en.wikipedia.org/wiki/Mapping_en:Infobox_poker_player?oldid=7401" .


