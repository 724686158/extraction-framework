### Main TriplesMap
####################
<>      a                      rr:TriplesMap ;
        dcterms:type           "templateMapping" ;
        rml:logicalSource      <./Mapping_en:Infobox_murderer/LogicalSource> ;
        rr:predicateObjectMap  <./Mapping_en:Infobox_murderer/SimplePropertyMapping/birthName/birthname> ,
			       <./Mapping_en:Infobox_murderer/SimplePropertyMapping/deathDate/death_date> ,
			       <./Mapping_en:Infobox_murderer/SimplePropertyMapping/foaf:name/name> ,
			       <./Mapping_en:Infobox_murderer/SimplePropertyMapping/country/country> ,
			       <./Mapping_en:Infobox_murderer/SimplePropertyMapping/birthDate/birth_date> ,
			       <./Mapping_en:Infobox_murderer/SimplePropertyMapping/deathPlace/death_place> ,
			       <./Mapping_en:Infobox_murderer/SimplePropertyMapping/birthPlace/birth_place> ;
        rr:subjectMap          <./Mapping_en:Infobox_murderer/SubjectMap> .


### Main SubjectMap
####################
<./Mapping_en:Infobox_murderer/SubjectMap>
        a            rr:SubjectMap ;
        rr:class     dul:Agent ,
		     wikidata:Q215627 ,
		     wikidata:Q5 ,
		     dbo:Murderer ,
		     dbo:Criminal ,
		     wikidata:Q16266334 ,
		     schema:Person ,
		     foaf:Person ,
		     dbo:Agent ,
		     dbo:Person ,
		     owl:Thing ,
		     dul:NaturalPerson ,
		     wikidata:Q2159907 ;
        rr:template  "http://en.dbpedia.org/resource/{wikititle}" ;
        rr:termType  rr:IRI .


### LogicalSource
##################

<./Mapping_en:Infobox_murderer/LogicalSource>
        a                         rml:LogicalSource ;
        rml:iterator              "Infobox" ;
        rml:referenceFormulation  ql:wikitext ;
        rml:source                "http://en.wikipedia.org/wiki/Mapping_en:Infobox_murderer?oldid=20812" .


