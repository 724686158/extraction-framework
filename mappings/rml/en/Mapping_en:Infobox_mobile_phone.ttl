### Main TriplesMap
####################
<>      a                      rr:TriplesMap ;
        dcterms:type           "templateMapping" ;
        rml:logicalSource      <./Mapping_en:Infobox_mobile_phone/LogicalSource> ;
        rr:predicateObjectMap  <./Mapping_en:Infobox_mobile_phone/SimplePropertyMapping/foaf:name/name> ,
			       <./Mapping_en:Infobox_mobile_phone/SimplePropertyMapping/operatingSystem/operatingsystem> ,
			       <./Mapping_en:Infobox_mobile_phone/SimplePropertyMapping/releaseDate/released> ,
			       <./Mapping_en:Infobox_mobile_phone/SimplePropertyMapping/manufacturer/manufacturer> ,
			       <./Mapping_en:Infobox_mobile_phone/SimplePropertyMapping/weight/weight> ,
			       <./Mapping_en:Infobox_mobile_phone/SimplePropertyMapping/operatingSystem/os> ,
			       <./Mapping_en:Infobox_mobile_phone/SimplePropertyMapping/status/status> ;
        rr:subjectMap          <./Mapping_en:Infobox_mobile_phone/SubjectMap> .


### Main SubjectMap
####################
<./Mapping_en:Infobox_mobile_phone/SubjectMap>
        a            rr:SubjectMap ;
        rr:class     dbo:Device ,
		     owl:Thing ;
        rr:template  "http://en.dbpedia.org/resource/{wikititle}" ;
        rr:termType  rr:IRI .


### LogicalSource
##################

<./Mapping_en:Infobox_mobile_phone/LogicalSource>
        a                         rml:LogicalSource ;
        rml:iterator              "Infobox" ;
        rml:referenceFormulation  ql:wikitext ;
        rml:source                "http://en.wikipedia.org/wiki/Mapping_en:Infobox_mobile_phone?oldid=16834" .


