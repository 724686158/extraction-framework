### Main TriplesMap
####################
<>      a                      rr:TriplesMap ;
        dcterms:type           "templateMapping" ;
        rml:logicalSource      <./Mapping_en:Infobox_Olympic_event/LogicalSource> ;
        rr:predicateObjectMap  <./Mapping_en:Infobox_Olympic_event/SimplePropertyMapping/event/event> ,
			       <./Mapping_en:Infobox_Olympic_event/SimplePropertyMapping/nextEvent/next> ,
			       <./Mapping_en:Infobox_Olympic_event/SimplePropertyMapping/previousEvent/prev> ,
			       <./Mapping_en:Infobox_Olympic_event/SimplePropertyMapping/silverMedalist/silver> ,
			       <./Mapping_en:Infobox_Olympic_event/SimplePropertyMapping/games/games> ,
			       <./Mapping_en:Infobox_Olympic_event/SimplePropertyMapping/bronzeMedalist/bronze> ,
			       <./Mapping_en:Infobox_Olympic_event/SimplePropertyMapping/goldMedalist/gold> ;
        rr:subjectMap          <./Mapping_en:Infobox_Olympic_event/SubjectMap> .


### Main SubjectMap
####################
<./Mapping_en:Infobox_Olympic_event/SubjectMap>
        a            rr:SubjectMap ;
        rr:class     dul:Event ,
		     schema:SportsEvent ,
		     wikidata:Q1656682 ,
		     dbo:SocietalEvent ,
		     dbo:Event ,
		     dbo:SportsEvent ,
		     schema:Event ,
		     dbo:OlympicEvent ,
		     dbo:Olympics ,
		     owl:Thing ;
        rr:template  "http://en.dbpedia.org/resource/{wikititle}" ;
        rr:termType  rr:IRI .


### LogicalSource
##################

<./Mapping_en:Infobox_Olympic_event/LogicalSource>
        a                         rml:LogicalSource ;
        rml:iterator              "Infobox" ;
        rml:referenceFormulation  ql:wikitext ;
        rml:source                "http://en.wikipedia.org/wiki/Mapping_en:Infobox_Olympic_event?oldid=20580" .


