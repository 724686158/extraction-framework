		     wikidata:Q215627 ,
		     dbo:Pope ,
		     wikidata:Q19546 ,
		     foaf:Person ;
        rr:template  "http://en.dbpedia.org/resource/{wikititle}" ;
        rr:termType  rr:IRI .


