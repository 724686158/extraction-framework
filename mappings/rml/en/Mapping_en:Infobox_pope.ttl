        dcterms:type           "templateMapping" ;
        rml:logicalSource      <./Mapping_en:Infobox_pope/LogicalSource> ;
        rr:predicateObjectMap  <./Mapping_en:Infobox_pope/SimplePropertyMapping/nationality/nationality> ,
			       <./Mapping_en:Infobox_pope/SimplePropertyMapping/birthDate/birth_date> ,
