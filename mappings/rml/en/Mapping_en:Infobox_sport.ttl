### Main TriplesMap
####################
<>      a                      rr:TriplesMap ;
        dcterms:type           "templateMapping" ;
        rml:logicalSource      <./Mapping_en:Infobox_sport/LogicalSource> ;
        rr:predicateObjectMap  <./Mapping_en:Infobox_sport/SimplePropertyMapping/category/category> ,
			       <./Mapping_en:Infobox_sport/SimplePropertyMapping/foaf:name/name> ,
			       <./Mapping_en:Infobox_sport/SimplePropertyMapping/teamSize/team> ,
			       <./Mapping_en:Infobox_sport/SimplePropertyMapping/equipment/ball> ,
			       <./Mapping_en:Infobox_sport/SimplePropertyMapping/foaf:nick/nickname> ,
			       <./Mapping_en:Infobox_sport/SimplePropertyMapping/equipment/equipment> ,
			       <./Mapping_en:Infobox_sport/SimplePropertyMapping/sportGoverningBody/union> ;
        rr:subjectMap          <./Mapping_en:Infobox_sport/SubjectMap> .


### Main SubjectMap
####################
<./Mapping_en:Infobox_sport/SubjectMap>
        a            rr:SubjectMap ;
        rr:class     dbo:Sport ,
		     wikidata:Q349 ,
		     dbo:Activity ,
		     d0:Activity ,
		     wikidata:Q1914636 ,
		     owl:Thing ;
        rr:template  "http://en.dbpedia.org/resource/{wikititle}" ;
        rr:termType  rr:IRI .


### LogicalSource
##################

<./Mapping_en:Infobox_sport/LogicalSource>
        a                         rml:LogicalSource ;
        rml:iterator              "Infobox" ;
        rml:referenceFormulation  ql:wikitext ;
        rml:source                "http://en.wikipedia.org/wiki/Mapping_en:Infobox_sport?oldid=10626" .


