        rr:predicateObjectMap  <./Mapping_en:Infobox_music_genre/SimplePropertyMapping/musicFusionGenre/fusiongenres> ,
			       <./Mapping_en:Infobox_music_genre/SimplePropertyMapping/instrument/instruments> ,
			       <./Mapping_en:Infobox_music_genre/SimplePropertyMapping/derivative/derivatives> ,
			       <./Mapping_en:Infobox_music_genre/SimplePropertyMapping/stylisticOrigin/stylistic_origins> ,
			       <./Mapping_en:Infobox_music_genre/SimplePropertyMapping/musicSubgenre/subgenres> ,
			       <./Mapping_en:Infobox_music_genre/SimplePropertyMapping/foaf:name/name> ;
        rr:subjectMap          <./Mapping_en:Infobox_music_genre/SubjectMap> .


### Main SubjectMap
####################
<./Mapping_en:Infobox_music_genre/SubjectMap>
        a            rr:SubjectMap ;
        rr:class     dbo:MusicGenre ,
		     wikidata:Q188451 ,
		     dul:Concept ,
		     dbo:Genre ,
		     dbo:TopicalConcept ,
		     owl:Thing ;
        rr:template  "http://en.dbpedia.org/resource/{wikititle}" ;
        rr:termType  rr:IRI .


