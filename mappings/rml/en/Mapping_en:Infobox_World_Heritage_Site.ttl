### Main TriplesMap
####################
<>      a                      rr:TriplesMap ;
        rml:logicalSource      <./Mapping_en:Infobox_World_Heritage_Site/LogicalSource> ;
        rr:predicateObjectMap  <./Mapping_en:Infobox_World_Heritage_Site/latitude> ,
			       <./Mapping_en:Infobox_World_Heritage_Site/longitude> ,
			       <./Mapping_en:Infobox_World_Heritage_Site/SimplePropertyMapping/id/ID> ,
			       <./Mapping_en:Infobox_World_Heritage_Site/SimplePropertyMapping/foaf:name/Name> ,
			       <./Mapping_en:Infobox_World_Heritage_Site/SimplePropertyMapping/foaf:homepage/Link> ,
			       <./Mapping_en:Infobox_World_Heritage_Site/SimplePropertyMapping/type/Type> ,
			       <./Mapping_en:Infobox_World_Heritage_Site/SimplePropertyMapping/region/Region> ;
        rr:subjectMap          <./Mapping_en:Infobox_World_Heritage_Site/SubjectMap> .


### Main SubjectMap
####################
<./Mapping_en:Infobox_World_Heritage_Site/SubjectMap>
        a            rr:SubjectMap ;
        rr:class     dbo:WorldHeritageSite ,
		     wikidata:Q9259 ,
		     dbo:Place ,
		     schema:Place ,
		     dbo:Location ,
		     owl:Thing ;
        rr:template  "http://en.dbpedia.org/resource/{wikititle}" ;
        rr:termType  rr:IRI .


### LogicalSource
##################

<./Mapping_en:Infobox_World_Heritage_Site/LogicalSource>
        a                         rml:LogicalSource ;
        rml:iterator              "Infobox" ;
        rml:referenceFormulation  ql:wikitext ;
        rml:source                "http://en.wikipedia.org/wiki/Mapping_en:Infobox_World_Heritage_Site?oldid=7473" .


