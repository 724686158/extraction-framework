### Main TriplesMap
####################
<>      a                      rr:TriplesMap ;
        dcterms:type           "templateMapping" ;
        rml:logicalSource      <./Mapping_en:Infobox_lighthouse/LogicalSource> ;
        rr:predicateObjectMap  <./Mapping_en:Infobox_lighthouse/SimplePropertyMapping/elevation/elevation> ,
			       <./Mapping_en:Infobox_lighthouse/latitude> ,
			       <./Mapping_en:Infobox_lighthouse/SimplePropertyMapping/buildingEndDate/yearbuilt> ,
			       <./Mapping_en:Infobox_lighthouse/longitude> ,
			       <./Mapping_en:Infobox_lighthouse/SimplePropertyMapping/height/height> ,
			       <./Mapping_en:Infobox_lighthouse/SimplePropertyMapping/foaf:name/name> ,
			       <./Mapping_en:Infobox_lighthouse/SimplePropertyMapping/location/location> ;
        rr:subjectMap          <./Mapping_en:Infobox_lighthouse/SubjectMap> .


### Main SubjectMap
####################
<./Mapping_en:Infobox_lighthouse/SubjectMap>
        a            rr:SubjectMap ;
        rr:class     wikidata:Q12518 ,
		     dbo:Tower ,
		     dbo:Lighthouse ,
		     schema:Place ,
		     dbo:Place ,
		     owl:Thing ,
		     wikidata:Q39715 ,
		     dbo:Location ,
		     dbo:ArchitecturalStructure ;
        rr:template  "http://en.dbpedia.org/resource/{wikititle}" ;
        rr:termType  rr:IRI .


### LogicalSource
##################

<./Mapping_en:Infobox_lighthouse/LogicalSource>
        a                         rml:LogicalSource ;
        rml:iterator              "Infobox" ;
        rml:referenceFormulation  ql:wikitext ;
        rml:source                "http://en.wikipedia.org/wiki/Mapping_en:Infobox_lighthouse?oldid=7340" .


