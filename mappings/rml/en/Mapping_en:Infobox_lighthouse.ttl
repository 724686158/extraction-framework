### Main TriplesMap
####################
<>      a                      rr:TriplesMap ;
        rdfs:comment           "Main Triples Map of Mapping en:Infobox lighthouse. This defines the generation of triples that are extracted from Infobox lighthouse." ;
        rdfs:label             "Mapping en:Infobox lighthouse (Triples Map)" ;
        rml:logicalSource      <./Mapping_en:Infobox_lighthouse/LogicalSource> ;
        rr:predicateObjectMap  <./Mapping_en:Infobox_lighthouse/latitude> ,
			       <./Mapping_en:Infobox_lighthouse/SimplePropertyMapping/0> ,
			       <./Mapping_en:Infobox_lighthouse/SimplePropertyMapping/4> ,
			       <./Mapping_en:Infobox_lighthouse/longitude> ,
			       <./Mapping_en:Infobox_lighthouse/SimplePropertyMapping/2> ,
			       <./Mapping_en:Infobox_lighthouse/SimplePropertyMapping/1> ,
			       <./Mapping_en:Infobox_lighthouse/SimplePropertyMapping/3> ;
        rr:subjectMap          <./Mapping_en:Infobox_lighthouse/SubjectMap> .


### Main SubjectMap
####################
<./Mapping_en:Infobox_lighthouse/SubjectMap>
        a             rr:SubjectMap ;
        rdfs:comment  "Main Subject Map of Mapping en:Infobox lighthouse. This defines the generation of the subject for the triples that are extracted from Infobox lighthouse." ;
        rdfs:label    "Mapping en:Infobox lighthouse (Subject Map)" ;
        rr:class      wikidata:Q12518 ,
		     dbo:Tower ,
		     dbo:Lighthouse ,
		     schema:Place ,
		     dbo:Place ,
		     owl:Thing ,
		     wikidata:Q39715 ,
		     dbo:Location ,
		     dbo:ArchitecturalStructure ;
        rr:template   "http://en.dbpedia.org/resource/{wikititle}" ;
        rr:termType   rr:IRI .


### LogicalSource
##################

<./Mapping_en:Infobox_lighthouse/LogicalSource>
        a                         rml:LogicalSource ;
        rml:iterator              "Infobox" ;
        rml:referenceFormulation  ql:wikitext ;
        rml:source                "http://en.wikipedia.org/wiki/Mapping_en:Infobox_lighthouse?oldid=7340" .


