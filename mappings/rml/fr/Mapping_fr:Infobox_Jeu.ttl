### Main TriplesMap
####################
<http://fr.dbpedia.org/resource/Mapping_fr:Infobox_Jeu>
        a                      rr:TriplesMap ;
        rml:logicalSource      <LogicalSource> ;
        rr:predicateObjectMap  <SimplePropertyMapping/9> ,
			       <SimplePropertyMapping/10> ,
			       <SimplePropertyMapping/11> ,
			       <SimplePropertyMapping/12> ,
			       <ConditionalMapping/type_equals_Jeu%20de%20soci%C3%A9t%C3%A9> ;
        rr:subjectMap          <SubjectMap> .


### Main SubjectMap
####################
<SubjectMap>  a      rr:SubjectMap ;
        rr:template  "http://en.dbpedia.org/resource/{wikititle}" ;
        rr:termType  rr:IRI .


### LogicalSource
##################

<LogicalSource>  a                rml:LogicalSource ;
        rml:iterator              "Infobox" ;
        rml:referenceFormulation  ql:wikitext ;
        rml:source                "http://fr.wikipedia.org/wiki/Mapping_fr:Infobox_Jeu?oldid=24166" .


