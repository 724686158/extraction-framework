### Main TriplesMap
####################
<>      a                      rr:TriplesMap ;
        rml:logicalSource      <./Mapping_fr:Infobox_Jeu/LogicalSource> ;
        rr:predicateObjectMap  <./Mapping_fr:Infobox_Jeu/SimplePropertyMapping/8> ,
			       <./Mapping_fr:Infobox_Jeu/SimplePropertyMapping/9> ,
			       <./Mapping_fr:Infobox_Jeu/SimplePropertyMapping/10> ,
			       <./Mapping_fr:Infobox_Jeu/SimplePropertyMapping/11> ,
			       <./Mapping_fr:Infobox_Jeu/ConditionalMapping/type_equals_Jeu%20de%20soci%C3%A9t%C3%A9> ;
        rr:subjectMap          <./Mapping_fr:Infobox_Jeu/SubjectMap> .


### Main SubjectMap
####################
<./Mapping_fr:Infobox_Jeu/SubjectMap>
        a            rr:SubjectMap ;
        rr:template  "http://en.dbpedia.org/resource/{wikititle}" ;
        rr:termType  rr:IRI .


### LogicalSource
##################

<./Mapping_fr:Infobox_Jeu/LogicalSource>
        a                         rml:LogicalSource ;
        rml:iterator              "Infobox" ;
        rml:referenceFormulation  ql:wikitext ;
        rml:source                "http://fr.wikipedia.org/wiki/Mapping_fr:Infobox_Jeu?oldid=24166" .


