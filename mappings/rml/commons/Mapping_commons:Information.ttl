			       <./Mapping_commons:Information/SimplePropertyMapping/15> ,
			       <./Mapping_commons:Information/SimplePropertyMapping/12> ,
			       <./Mapping_commons:Information/SimplePropertyMapping/2> ;
        rr:subjectMap          <./Mapping_commons:Information/SubjectMap> .


### Main SubjectMap
####################
<./Mapping_commons:Information/SubjectMap>
        a            rr:SubjectMap ;
        rr:class     dbo:Work ,
		     schema:CreativeWork ,
		     wikidata:Q386724 ,
		     owl:Thing ;
        rr:template  "http://en.dbpedia.org/resource/{wikititle}" ;
        rr:termType  rr:IRI .


### LogicalSource
##################

<./Mapping_commons:Information/LogicalSource>
        a                         rml:LogicalSource ;
        rml:iterator              "Infobox" ;
        rml:referenceFormulation  ql:wikitext ;
        rml:source                "http://commons.wikimedia.org/wiki/Mapping_commons:Information?oldid=39426" .


