### Main TriplesMap
####################
<>      a                      rr:TriplesMap ;
        rml:logicalSource      <./Mapping_commons:Chemical_structure_verified/LogicalSource> ;
        rr:predicateObjectMap  <./Mapping_commons:Chemical_structure_verified/SimplePropertyMapping/0> ,
			       <./Mapping_commons:Chemical_structure_verified/SimplePropertyMapping/1> ,
			       <./Mapping_commons:Chemical_structure_verified/SimplePropertyMapping/2> ,
			       <./Mapping_commons:Chemical_structure_verified/SimplePropertyMapping/3> ;
        rr:subjectMap          <./Mapping_commons:Chemical_structure_verified/SubjectMap> .


### Main SubjectMap
####################
<./Mapping_commons:Chemical_structure_verified/SubjectMap>
        a            rr:SubjectMap ;
        rr:class     dbo:ChemicalSubstance ,
		     dul:ChemicalObject ,
		     owl:Thing ;
        rr:template  "http://en.dbpedia.org/resource/{wikititle}" ;
        rr:termType  rr:IRI .


### LogicalSource
##################

<./Mapping_commons:Chemical_structure_verified/LogicalSource>
        a                         rml:LogicalSource ;
        rml:iterator              "Infobox" ;
        rml:referenceFormulation  ql:wikitext ;
        rml:source                "http://commons.wikimedia.org/wiki/Mapping_commons:Chemical_structure_verified?oldid=37560" .


