### Main TriplesMap
####################
<>      a                      rr:TriplesMap ;
        rml:logicalSource      <Manga/LogicalSource> ;
        rr:predicateObjectMap  <Manga/SimplePropertyMapping/0> ,
			       <Manga/SimplePropertyMapping/1> ,
			       <Manga/SimplePropertyMapping/2> ,
			       <Manga/SimplePropertyMapping/3> ,
			       <Manga/SimplePropertyMapping/4> ,
			       <Manga/SimplePropertyMapping/5> ;
        rr:subjectMap          <Manga/SubjectMap> .


### Main SubjectMap
####################
<Manga/SubjectMap>  a  rr:SubjectMap ;
        rr:class     dbo:Work ,
		     dbo:Manga ,
		     dbo:WrittenWork ,
		     schema:CreativeWork ,
		     wikidata:Q8274 ,
		     dbo:Comic ,
		     wikidata:Q386724 ,
		     wikidata:Q245068 ,
		     owl:Thing ;
        rr:template  "http://en.dbpedia.org/resource/{wikititle}" ;
        rr:termType  rr:IRI .


### LogicalSource
##################

<Manga/LogicalSource>
        a                         rml:LogicalSource ;
        rml:iterator              "Infobox" ;
        rml:referenceFormulation  ql:wikitext ;
        rml:source                "http://ja.wikipedia.org/wiki/Mapping_ja:Infobox_animanga/Manga?oldid=19376" .


###########
# Mappings
###########

### Predicate Object Map
#########################
<Manga/SimplePropertyMapping/5>
        a             rr:PredicateObjectMap ;
        rdfs:comment  "A PredicateObjectMap of the main TriplesMap that defines the generation of the predicate and object of a triple. The object value will be the result of a SimplePropertyFunction." ;
        rdfs:label    "SimplePropertyMapping (PredicateObjectMap)." ;
        rr:objectMap  <Manga/SimplePropertyMapping/5/FTM> ;
        rr:predicate  dbo:completionDate .

### Function Term Map
<Manga/SimplePropertyMapping/5/FTM>
        a                   fnml:FunctionTermMap ;
        fnml:functionValue  <Manga/SimplePropertyMapping/5/FTM/FV> .

### Function Execution Mapping
<Manga/SimplePropertyMapping/5/FTM/FV>
        a                      rr:TriplesMap ;
        rdfs:comment           "TriplesMap that generates and returns execution triples that can be executed by the SimplePropertyFunction." ;
        rdfs:label             "SimplePropertyMapping TriplesMap" ;
        rml:logicalSource      <Manga/LogicalSource> ;
        rr:predicateObjectMap  <Manga/Function/SimplePropertyFunction> ,
			       <Manga/SimplePropertyMapping/5/FTM/FV/propertyParameterPOM> ,
			       <Manga/SimplePropertyMapping/5/FTM/FV/ontologyPropertyParameterPOM> ;
        rr:subjectMap          <Manga/SubjectMap/Function> .

### Parameters
<Manga/SimplePropertyMapping/5/FTM/FV/ontologyPropertyParameterPOM>
        a             rr:PredicateObjectMap ;
        rdfs:comment  "A Predicate Object Map for generating and returning the predicate and/or object of a parameter triple." ;
        rdfs:label    "Parameter Predicate Object Map" ;
        rr:object     "completionDate" ;
        rr:predicate  dbf:ontologyPropertyParameter .

<Manga/SimplePropertyMapping/5/FTM/FV/propertyParameterPOM>
        a             rr:PredicateObjectMap ;
        rdfs:comment  "A Predicate Object Map for generating and returning the predicate and/or object of a parameter triple." ;
        rdfs:label    "Parameter Predicate Object Map" ;
        rr:objectMap  <Manga/SimplePropertyMapping/5/FTM/FV/propertyParameterPOM/ObjectMap> ;
        rr:predicate  dbf:propertyParameter .

### References
<Manga/SimplePropertyMapping/5/FTM/FV/propertyParameterPOM/ObjectMap>
        a              rr:ObjectMap ;
        rdfs:comment   "An Object Map for generating and returning the object of a parameter triple. This Object Map refers to a value of a key in an infoboxy with the rml:reference property." ;
        rdfs:label     "Parameter Object Map" ;
        rml:reference  "終了日" .


