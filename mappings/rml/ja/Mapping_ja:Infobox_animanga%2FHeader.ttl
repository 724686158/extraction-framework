### Main TriplesMap
####################
<>      a                      rr:TriplesMap ;
        rml:logicalSource      <Header/LogicalSource> ;
        rr:predicateObjectMap  <Header/SimplePropertyMapping/0> ,
			       <Header/SimplePropertyMapping/1> ,
			       <Header/SimplePropertyMapping/2> ;
        rr:subjectMap          <Header/SubjectMap> .


### Main SubjectMap
####################
<Header/SubjectMap>  a  rr:SubjectMap ;
        rr:class     dbo:Anime ,
		     wikidata:Q1107 ,
		     wikidata:Q627603 ,
		     dbo:Cartoon ,
		     wikidata:Q386724 ,
		     owl:Thing ,
		     dbo:Work ,
		     schema:CreativeWork ;
        rr:template  "http://en.dbpedia.org/resource/{wikititle}" ;
        rr:termType  rr:IRI .


### LogicalSource
##################

<Header/LogicalSource>
        a                         rml:LogicalSource ;
        rml:iterator              "Infobox" ;
        rml:referenceFormulation  ql:wikitext ;
        rml:source                "http://ja.wikipedia.org/wiki/Mapping_ja:Infobox_animanga/Header?oldid=19509" .


###########
# Mappings
###########

### Predicate Object Map
#########################
<Header/SimplePropertyMapping/2>
        a             rr:PredicateObjectMap ;
        rdfs:comment  "A PredicateObjectMap of the main TriplesMap that defines the generation of the predicate and object of a triple. The object value will be the result of a SimplePropertyFunction." ;
        rdfs:label    "SimplePropertyMapping (PredicateObjectMap)." ;
        rr:objectMap  <Header/SimplePropertyMapping/2/FTM> ;
        rr:predicate  dbo:picture .

### Function Term Map
<Header/SimplePropertyMapping/2/FTM>
        a                   fnml:FunctionTermMap ;
        fnml:functionValue  <Header/SimplePropertyMapping/2/FTM/FV> .

### Function Execution Mapping
<Header/SimplePropertyMapping/2/FTM/FV>
        a                      rr:TriplesMap ;
        rdfs:comment           "TriplesMap that generates and returns execution triples that can be executed by the SimplePropertyFunction." ;
        rdfs:label             "SimplePropertyMapping TriplesMap" ;
        rml:logicalSource      <Header/LogicalSource> ;
        rr:predicateObjectMap  <Header/Function/SimplePropertyFunction> ,
			       <Header/SimplePropertyMapping/2/FTM/FV/propertyParameterPOM> ,
			       <Header/SimplePropertyMapping/2/FTM/FV/ontologyPropertyParameterPOM> ;
        rr:subjectMap          <Header/SubjectMap/Function> .

### Parameters
<Header/SimplePropertyMapping/2/FTM/FV/propertyParameterPOM>
        a             rr:PredicateObjectMap ;
        rdfs:comment  "A Predicate Object Map for generating and returning the predicate and/or object of a parameter triple." ;
        rdfs:label    "Parameter Predicate Object Map" ;
        rr:objectMap  <Header/SimplePropertyMapping/2/FTM/FV/propertyParameterPOM/ObjectMap> ;
        rr:predicate  dbf:propertyParameter .

<Header/SimplePropertyMapping/2/FTM/FV/ontologyPropertyParameterPOM>
        a             rr:PredicateObjectMap ;
        rdfs:comment  "A Predicate Object Map for generating and returning the predicate and/or object of a parameter triple." ;
        rdfs:label    "Parameter Predicate Object Map" ;
        rr:object     "picture" ;
        rr:predicate  dbf:ontologyPropertyParameter .

### References
<Header/SimplePropertyMapping/2/FTM/FV/propertyParameterPOM/ObjectMap>
        a              rr:ObjectMap ;
        rdfs:comment   "An Object Map for generating and returning the object of a parameter triple. This Object Map refers to a value of a key in an infoboxy with the rml:reference property." ;
        rdfs:label     "Parameter Object Map" ;
        rml:reference  "画像" .


