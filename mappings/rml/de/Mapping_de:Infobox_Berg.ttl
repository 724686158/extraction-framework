        rr:predicateObjectMap  <./Mapping_de:Infobox_Berg/Function/LatitudeFunction> , <./Mapping_de:Infobox_Berg/latitude/FTM/ParameterPOM> ;
        rr:subjectMap          <./Mapping_de:Infobox_Berg/SubjectMap/Function> .

