### Main TriplesMap
####################
<>      a                      rr:TriplesMap ;
        rml:logicalSource      <./Mapping_de:Infobox_Berg/LogicalSource> ;
        rr:predicateObjectMap  <./Mapping_de:Infobox_Berg/latitude> ,
			       <./Mapping_de:Infobox_Berg/SimplePropertyMapping/10> ,
			       <./Mapping_de:Infobox_Berg/SimplePropertyMapping/5> ,
			       <./Mapping_de:Infobox_Berg/SimplePropertyMapping/1> ,
			       <./Mapping_de:Infobox_Berg/SimplePropertyMapping/4> ,
			       <./Mapping_de:Infobox_Berg/SimplePropertyMapping/7> ,
			       <./Mapping_de:Infobox_Berg/SimplePropertyMapping/0> ,
			       <./Mapping_de:Infobox_Berg/SimplePropertyMapping/3> ,
			       <./Mapping_de:Infobox_Berg/SimplePropertyMapping/9> ,
			       <./Mapping_de:Infobox_Berg/SimplePropertyMapping/11> ,
			       <./Mapping_de:Infobox_Berg/longitude> ,
			       <./Mapping_de:Infobox_Berg/SimplePropertyMapping/6> ,
			       <./Mapping_de:Infobox_Berg/SimplePropertyMapping/2> ,
			       <./Mapping_de:Infobox_Berg/SimplePropertyMapping/8> ;
        rr:subjectMap          <./Mapping_de:Infobox_Berg/SubjectMap> .


### Main SubjectMap
####################
<./Mapping_de:Infobox_Berg/SubjectMap>
        a            rr:SubjectMap ;
        rr:class     schema:Mountain ,
		     dbo:Mountain ,
		     dbo:Place ,
		     dbo:NaturalPlace ,
		     dbo:Location ,
		     schema:Place ,
		     wikidata:Q8502 ,
		     owl:Thing ;
        rr:template  "http://en.dbpedia.org/resource/{wikititle}" ;
        rr:termType  rr:IRI .


### LogicalSource
##################

<./Mapping_de:Infobox_Berg/LogicalSource>
        a                         rml:LogicalSource ;
        rml:iterator              "Infobox" ;
        rml:referenceFormulation  ql:wikitext ;
        rml:source                "http://de.wikipedia.org/wiki/Mapping_de:Infobox_Berg?oldid=47474" .


