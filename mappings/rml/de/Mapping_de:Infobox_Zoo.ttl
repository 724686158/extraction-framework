        rr:predicateObjectMap  <./Mapping_de:Infobox_Zoo/Function/LatitudeFunction> , <./Mapping_de:Infobox_Zoo/latitude/FTM/ParameterPOM> ;
        rr:subjectMap          <./Mapping_de:Infobox_Zoo/SubjectMap/Function> .

