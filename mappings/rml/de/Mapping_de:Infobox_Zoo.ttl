### Main TriplesMap
####################
<>      a                      rr:TriplesMap ;
        rml:logicalSource      <./Mapping_de:Infobox_Zoo/LogicalSource> ;
        rr:predicateObjectMap  <./Mapping_de:Infobox_Zoo/latitude> ,
			       <./Mapping_de:Infobox_Zoo/SimplePropertyMapping/3> ,
			       <./Mapping_de:Infobox_Zoo/SimplePropertyMapping/0> ,
			       <./Mapping_de:Infobox_Zoo/SimplePropertyMapping/2> ,
			       <./Mapping_de:Infobox_Zoo/SimplePropertyMapping/5> ,
			       <./Mapping_de:Infobox_Zoo/SimplePropertyMapping/4> ,
			       <./Mapping_de:Infobox_Zoo/SimplePropertyMapping/1> ,
			       <./Mapping_de:Infobox_Zoo/longitude> ;
        rr:subjectMap          <./Mapping_de:Infobox_Zoo/SubjectMap> .


### Main SubjectMap
####################
<./Mapping_de:Infobox_Zoo/SubjectMap>
        a            rr:SubjectMap ;
        rr:class     schema:Place ,
		     dbo:Location ,
		     owl:Thing ,
		     dbo:ArchitecturalStructure ,
		     dbo:Place ,
		     wikidata:Q43501 ,
		     dbo:Zoo ;
        rr:template  "http://en.dbpedia.org/resource/{wikititle}" ;
        rr:termType  rr:IRI .


### LogicalSource
##################

<./Mapping_de:Infobox_Zoo/LogicalSource>
        a                         rml:LogicalSource ;
        rml:iterator              "Infobox" ;
        rml:referenceFormulation  ql:wikitext ;
        rml:source                "http://de.wikipedia.org/wiki/Mapping_de:Infobox_Zoo?oldid=47296" .


