        rdfs:label    "SimplePropertyMapping (PredicateObjectMap)." ;
        rr:objectMap  <./Mapping_de:Infobox_Drama/SimplePropertyMapping/9/FTM> ;
        rr:predicate  dbo:settingOfPlay .

