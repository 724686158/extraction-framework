     uriConcat(?fv,'/languageParam', ?n) ->
                                                    (?fv rr:predicateObjectMap ?n),
                                                    (?n rdf:type rr:PredicateObjectMap),
                                                    (?n rr:predicate dbf:languageParameter),
                                                    (?n rr:object "{LANG}")]

